{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "lax/alertbox",
  "version": "0.1.0",
  "title": "Alertbox",
  "category": "widgets",
  "icon": "admin-comments",
  "description": "Alert box block with block transforms support",
  "supports": {
    "html": false,
    "anchor": true
  },
  "attributes": {
    "align": {
      "type": "string"
    },
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": ""
    }
  },
  "textdomain": "alertbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}